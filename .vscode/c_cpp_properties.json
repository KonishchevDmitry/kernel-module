{
    "version": 4,
    "configurations": [
        {
            "name": "Linux",
            "defines": [
                "__KERNEL__"
            ],
            "includePath": [
                "${workspaceFolder}",
                "${workspaceFolder}../kernel/include",
                "${workspaceFolder}../kernel/include/uapi",
                "${workspaceFolder}../kernel/include/generated/uapi",
                "${workspaceFolder}../kernel/arch/x86/include",
                "${workspaceFolder}../kernel/arch/x86/include/uapi",
                "${workspaceFolder}../kernel/arch/x86/include/generated",
                "${workspaceFolder}../kernel/arch/x86/include/generated/uapi",
                "${workspaceFolder}../kernel/tools/include"
            ],
            "compileCommands": "${workspaceFolder}/.vscode/compile_commands.json",
            "compilerPath": "/usr/bin/gcc",
            "cStandard": "gnu17",
            "intelliSenseMode": "linux-gcc-x64",
            "configurationProvider": "ms-vscode.makefile-tools",
            "dotConfig": "${workspaceFolder}/../kernel/.config",
            "forcedInclude": [
                "${workspaceFolder}/../kernel/include/linux/kconfig.h",
                "${workspaceFolder}/../kernel/include/linux/compiler_types.h"
            ]
        }
    ]
}